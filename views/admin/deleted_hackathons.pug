doctype html
html(lang='en')
	head
		include ./partials/head_preamble.pug	
	body
		include ./partials/navbar.pug
		.container
			.starter-template
				h1 Deleted Hackathons
				p.lead These hackathons are deleted and only serve for logging purposes. If you want to add them, you'll have to do it manually.
				.bs-example
					table.table.table-striped.lead
						thead
							tr
								th Naam
								th Waar
								th Wanneer
						tbody
							each hackathon in hackathons
								tr
									td 
										a(href="http://www."+hackathon.url, target='_blank') #{hackathon.name}
									td #{hackathon.location.city}									
									td #{hackathon.start_date}
		include ./partials/scripts.pug